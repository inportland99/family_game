<%= simple_form_for(@user) do |f| %>
    <%= f.input :name %>
    <%= f.input :role, collection: ["Admin", "Parent", "Player"] %>
    <%= f.input :email %>
    <%= f.input :password %>
    <%= f.input :password_confirmation %>
    <%# f.input :admin %>
    <%= f.button :submit, class: "m-btn" %>
<% end %>
